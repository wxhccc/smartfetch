{
  "name": "@wxhccc/smartfetch",
  "version": "2.0.1",
  "description": "an easy use api plugin for vue and react",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.min.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "rollup -w -c --environment NODE_ENV:development",
    "build": "rollup -c && npm run build:types",
    "pub": "npm run build && npm publish",
    "build:types": "tsc",
    "test": "jest",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wxhccc/smartfetch.git"
  },
  "keywords": [
    "api",
    "request",
    "mvvm",
    "fetch"
  ],
  "author": "wxhccc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wxhccc/smartfetch/issues"
  },
  "homepage": "https://github.com/wxhccc/smartfetch#readme",
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/jest": "^26.0.22",
    "@typescript-eslint/parser": "^4.18.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "eslint": "^7.22.0",
    "husky": ">=6",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "rollup": "^2.42.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.2.3",
    "vue": "^3.0.11",
    "vue2": "npm:vue"
  },
  "dependencies": {
    "query-string": "^7.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ],
    "*.{js,css,md}": "prettier --write"
  },
  "peerDependencies": {
    "axios": "^0.21.1"
  }
}
